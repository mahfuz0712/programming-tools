{
  "name": "programming-tools",
  "version": "1.0.0",
  "description": "A cli tool to inspect how many compilers or interpreters are installed of how many programming languages and their tools respectively",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/index.js",
    "build-windows": "pkg src/index.js --targets node16-windows-x64 --output dist/pt.exe",
    "build-linux": "pkg src/index.js --targets node16-linux-x64 --output dist/pt",
    "build-macos": "pkg src/index.js --targets node16-macos-x64 --output dist/pt_mac"
  },
  "keywords": [
    "Programming Tools, CyberSecurity",
    "JavaScript",
    "Node JS"
  ],
  "author": "Mohammad Mahfuz Rahman",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "ora": "^5.4.1",
    "strip-ansi": "^7.1.0",
    "table": "^6.9.0"
  },
  "license": "ISC"
}
